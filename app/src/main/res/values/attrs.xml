<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--

        Theme Attributes

            These custom attributes are represent color categories which can be overridden in each theme and used
            throughout the app. The access syntax is ?attr/colorName.

    -->


    <!-- System Colors -->
    <attr name="themeColorPrimary" format="reference|color" />
    <attr name="themeColorPrimaryDark" format="reference|color" />
    <attr name="themeColorAccent" format="reference|color" />

    <!-- Control Colors-->
    <attr name="themeColorTextHandle" format="reference|color" />
    <attr name="themeColorTextHighlight" format="reference|color" />
    <attr name="themeColorTextHint" format="reference|color" />
    <attr name="themeColorRipplePrimary" format="reference|color" />


</resources>